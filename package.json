{
  "name": "now-bindist",
  "version": "0.3.2",
  "description": "The command line interface for Now",
  "repository": "zeit/now-cli",
  "license": "MIT",
  "bin": {
    "now": "dist/now.exe",
    "now-update": "update.js"
  },
  "scripts": {
    "postinstall": "node postinstall.js",
    "prepublish": "in-install || (npm run webpack && cp /dev/null dist/now.exe)",
    "webpack": "webpack --context download --config download/webpack.js"
  },
  "keywords": [
    "deploy",
    "cloud",
    "platform",
    "sharing"
  ],
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "in-publish": "2.0.0",
    "node-fetch": "1.6.3",
    "progress": "2.0.0",
    "webpack": "2.5.1"
  }
}
